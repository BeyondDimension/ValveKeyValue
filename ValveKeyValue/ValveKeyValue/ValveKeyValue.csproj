<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.3</TargetFrameworks>
    <Product>Valve KeyValue Library</Product>
    <Description>Library to parse and write Valve KeyValue formats</Description>
    <Copyright>Copyright © SteamDatabase 2017</Copyright>
    <Company>SteamDatabase</Company>
    <Authors>SteamDatabase</Authors>
    <RepositoryUrl>https://github.com/SteamDatabase/ValveKeyValue</RepositoryUrl>
    <PackageProjectUrl>https://github.com/SteamDatabase/ValveKeyValue</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Reflection.TypeExtensions" Version="4.3.0" />
    <PackageReference Include="System.Runtime.Serialization.Formatters" Version="4.3.0" />
  </ItemGroup>

</Project>